#include <iostream>
using namespace std;
int main()
{
    int i, n;
    cin>>n;
    int a[n];
    for (i=0; i<n; i++)
    {
        cin>>a[i];
    }
    for (i=0; i<n-1; i++)
    {
        if ( a[0]>a[i+1])
        {
            cout<<a[i+1]<<" "<<a[0]<<" ";
        } else if (a[0]<a[i+1] || a[0]==a[i+1])
        {
            cout<<a[i+1]<<" ";
        }
    }
    return 0;
}
