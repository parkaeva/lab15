#include <iostream>

using namespace std;

int main()
{
    int i, n, s;
    cin>>n;
    int a[n];
    for (i=0; i<n; i++)
    {
        cin>>a[i];
    }
    for (i=0; i<n; i++)
    {
        if (a[i]%2!=0)
        {
            s=a[i];
        }
    }
    for (i=0; i<n; i++)
    {
        if (a[i]%2!=0)
        {
            a[i]+=s;;
        }
        cout<<a[i]<<" ";
    }
    return 0;
}
