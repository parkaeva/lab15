#include <iostream>
using namespace std;
int main()
{
    int i, n, max, min, t, r;
    cin>>n;
    int a[n];
    max=-1;
    min=10000000;
    for (i=0; i<n; i++)
    {
        cin>>a[i];
    }
    for (i=0; i<n; i++)
    {
        if (a[i]>max)
        {
            max=a[i];
            t=i;
        }
    }
    for (i=0; i<n; i++)
    {
        if (a[i]<min)
        {
            min=a[i];
            r=i;
        }
    }
    for (i=0; i<n; i++)
    {
        if (i>r && i<t || i>t && i<r)
        {
            a[i]=0;
        }
        cout<<a[i]<<" ";
    }
    return 0;
}
